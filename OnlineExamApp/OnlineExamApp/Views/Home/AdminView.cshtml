@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Admin Dashboard</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 20px;
        }

        h1 {
            text-align: center;
            margin-bottom: 20px;
        }

        .dashboard-link {
            display: block;
            text-align:justify;
            margin-bottom: 10px;
        }

            .dashboard-link a {
                text-decoration: none;
                color: #333;
                background-color: #f5f5f5;
                padding: 10px 20px;
                border-radius: 10px;
            }

                .dashboard-link a:hover {
                    background-color: #ebebeb;
                }
    </style>
</head>
<body>
    <h1>Admin Dashboard</h1>

    <div class="dashboard-link">
        <a asp-controller="Admin" asp-action="StudentList">Student List</a>
    </div>
    <br />
    <br />
    <div class="dashboard-link">
        <a asp-controller="PaperSetterList" asp-action="SetterList">Paper Setter List</a>
    </div>
    <br />
    <br />
    <div class="dashboard-link">
        <a asp-controller="Enrollment" asp-action="GetAllEnroll">Enrollment List</a>
    </div>
    <br />
    <br />
    <div class="dashboard-link">
        <a asp-controller="Admin" asp-action="QuestionBankList">QuestionBank List</a>
    </div>
    <br />
    <br />
    <div class="dashboard-link">
        <a asp-controller="Admin" asp-action="ExamList">Exam List</a>
    </div>
    <br />
    <br />

</body>
</html>



